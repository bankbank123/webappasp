@model BasisCRUD.Models.db.Publisher;
@{
    ViewData["Title"] = "Create";
}

<h1>เพิ่มข้อมูลชื่อผู้จัดพิมพ์</h1>

<form asp-controller="Publisher" asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="PublishName"></label>
        <input type="text" asp-for="PublishName" class="form-control"/>
        <span asp-validation-for="PublishName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ContactName"></label>
        <input type="text" asp-for="ContactName" class="form-control"/>
        <span asp-validation-for="ContactName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Address"></label>
        <input type="text" asp-for="Address" class="form-control"/>
        <span asp-validation-for="Address" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Telephone"></label>
        <input type="text" asp-for="Telephone" class="form-control"/>
        <span asp-validation-for="Telephone" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
    <button type="reset" class="btn btn-primary">Reset</button>
</form>